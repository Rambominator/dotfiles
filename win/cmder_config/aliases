e.=explorer .
ls=ls --color $*
pwd=cd
clear=cls

gunzip=gzip -d $*
wgetc= wget --no-check-certificate $*

g=git $*
gh=git log --oneline --all --graph --decorate  $*
gs=git status $*
ga=git add $*
gb=git branch $*
gc=git commit $*
gd=git diff --color $*
go=git checkout $*
gl=git pull $*
gp=git push $*
gca=git commit -a $*

h=hg $*
hs=hg status $*
hd=hg diff $*
hl=hg pull $*
hp=hg push -r . $*
hgl=hg hist -b . -l 10 $*
